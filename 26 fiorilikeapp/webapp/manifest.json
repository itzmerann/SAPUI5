{
	"version": "1.12.0",
	"sap.ui": {
		"_version": "1.3.0",
		"technology": "UI5",
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": ["sap_fiori3", "sap_belize"]
	},
	"sap.app": {
		"_version": "1.3.0",
		"id": "emc.fin.ar",
		"type": "application",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"tags": {
			"keywords": ["{{fruits}}"]
		},
		"ach": "FIN-ACC-AR"
	},
	"sap.ui5": {
		"_version": "1.2.0",
		"dependencies": {
			"minUI5Version": "1.60",
			"libs": {
				"sap.m": {
					"minVersion": ""
				},
				"sap.ui.layout": {
					"minVersion": ""
				}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"resources": {
			"css": [{
				"uri": "css/style.css",
				"id": ""
			}]
        },
        "rootView":{
            "id": "idAppView",
            "viewName": "emc.fin.ar.view.App",
            "type": "XML"
        },
        "routing":{
            "config":{
                "routerClass": "sap.m.routing.Router",
                "viewPath": "emc.fin.ar.view",
                "viewType": "XML",
                "controlId": "idAppCon"
            },
            "routes": [
                {
                    "pattern": "",
                    "name": "start",
                    "target":["master", "empty"]
                }
            ],
            "targets": {
                "master":{
                    "viewName": "View1",
                    "controlAggregation": "masterPages",
                    "viewId": "idView1"
                },
                "empty":{
                    "viewName": "Avengers",
                    "controlAggregation": "detailPages",
                    "viewId": "idEmpty"
                }
            }
        },
        "models": {
            "i18n": {
                "type":"sap.ui.model.resource.ResourceModel",
                "settings":{
                    "bundleName": "i18n/i18n.properties"
                }
            },
            "":{
                "type": "sap.ui.model.json.JSONModel",
                "uri": "models/fruitdata.json"
            }
        }
	},
	"_version": "1.9.0"
}